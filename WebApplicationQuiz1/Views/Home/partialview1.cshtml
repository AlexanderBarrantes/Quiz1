@model IEnumerable<WebApplicationQuiz1.Models.partialview>
@using WebApplicationQuiz1.Models


@if (Model != null)
{
    <div class="grid">
        <table cellspacing="0" width="80%">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                    <th>
                        Description
                    </th>
                    <th>
                        Details
                    </th>
                    <th>
                        Date
                    </th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td align="left">
                            @item.Name
                        </td>
                        <td align="left">
                            @item.Description
                        </td>
                        <td align="left">
                            @item.Details
                        </td>
                        <td align="left">
                            @item.Date
                        </td>

                    </tr>
                }
            </tbody>
        </table>

    </div>

}


